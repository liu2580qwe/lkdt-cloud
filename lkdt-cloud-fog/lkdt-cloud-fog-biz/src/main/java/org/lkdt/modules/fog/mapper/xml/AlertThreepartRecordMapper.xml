<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lkdt.modules.fog.mapper.AlertThreepartRecordMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  zc_alert_threepart_record 
		WHERE
			 alert_threepart_id = #{mainId} 	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.lkdt.modules.fog.entity.AlertThreepartRecord">
		SELECT * 
		FROM  zc_alert_threepart_record
		WHERE
			 alert_threepart_id = #{mainId} 	</select>
</mapper>
