<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lkdt.modules.radar.mapper.ZcLdWindMapper">

	<select id="selectByEpId" parameterType="java.lang.String" resultType="org.lkdt.modules.radar.entity.ZcLdWind">
		select * from zc_ld_wind where id IN (SELECT wind_id FROM zc_ld_wind_radar_relation WHERE radar_id = #{epId})
	</select>
	
</mapper>