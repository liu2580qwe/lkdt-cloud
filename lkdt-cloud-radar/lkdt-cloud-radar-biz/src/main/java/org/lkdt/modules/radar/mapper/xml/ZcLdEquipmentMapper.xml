<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lkdt.modules.radar.mapper.ZcLdEquipmentMapper">

	<!-- 雷达列表 -->
	<select id="queryEquipment" resultType="org.lkdt.modules.radar.entity.ZcLdEquipment">
		select * 
		from zc_ld_radar_equipment
		where equ_type in ('001','002') 
		order by id
	</select>
	
</mapper>